<?xml version='1.0' encoding='utf-8'?>
<widget id="tld.company.app" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Starter</name>
    <description>A Cordova/React/Typescript Starter</description>
    <author email="daniel.d.steren@gmail.com" href="https://github.com/dansteren">Dan Steren</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~7.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
